/var      	/var 	 ext4 	 defaults,bind,nosuid 			 1 2
/boot		/boot	 ext4	 defaults,bind,nosuid,nodev,noexec	 1 2
/home		/home	 ext4	 defaults,bind,nosuid,nodev		 1 2
tmpfs           /tmp     tmpfs   defaults,nosuid,nodev,noexec            1 2
tmpfs		/dev/shm tmpfs   defaults,nosuid,nodev,noexec		 0 0
proc            /proc    proc    nosuid,nodev,noexec,gid=proc 		 0 0
